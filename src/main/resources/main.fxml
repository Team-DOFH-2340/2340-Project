<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<GridPane xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="25" vgap="10"
		  fx:controller="controller.MainScreenController">

    <padding>
        <Insets top="25" right="25" bottom="10" left="25"/>
    </padding>

    <Button text="Edit Profile" fx:id="edit"
            onMouseClicked="#editProfile"
            GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <Button text="Log Out" fx:id="logout"
            onMouseClicked="#logout"
            GridPane.columnIndex="1" GridPane.rowIndex="0"/>
    <Button text="Source Report" fx:id="source_report"
            onMouseClicked="#source_report"
            GridPane.columnIndex="0" GridPane.rowIndex="1"/>
</GridPane>